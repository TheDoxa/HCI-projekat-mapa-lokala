<UserControl x:Class="BarManager.BarView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BarManager"
             mc:Ignorable="d"
             Width="60"
             Height="60"
             d:DesignHeight="60" d:DesignWidth="60" Background="Bisque">
    <Grid>
        <Label Name="Naziv" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="8" Width="60" Height="30" HorizontalContentAlignment="Center" />
        <Label Name="Opis" Content="Label" HorizontalAlignment="Left" Margin="0,30,0,0" VerticalAlignment="Top" FontSize="8" Width="60" Height="30" HorizontalContentAlignment="Center" />

    </Grid>
</UserControl>
